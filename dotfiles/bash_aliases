alias e="exit"
alias l="ls -l"
alias ll="ls -la"
alias branch-done="git ch master && git pull -p && git delete-merged-branches"
alias nt="gnome-terminal"
alias mdir=". makedir"
alias rebase="git rebase -i HEAD~"
