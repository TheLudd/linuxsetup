alias apap="sudo apt-get update && sudo apt-get upgrade && sudo apt-get autoremove && sudo apt-get autoclean"
alias e="exit"
alias l="ls -l"
alias ll="ls -la"
alias branch-done="git ch master && git pull -p && git delete-merged-branches"
alias nt="gnome-terminal"
alias mdir=". makedir"
alias rebase="git rebase -i HEAD~"
alias upnpm="sudo npm i -g npm && npm completion | sudo tee /etc/bash_completion.d/npm"
alias mt="cd ~/mediatool && zsh"
alias clock="killall unity-panel-service"
